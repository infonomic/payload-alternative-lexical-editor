#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/tony/Clients/Infonomic/Projects/Payload/payload-alternative-lexical-editor/node_modules/.pnpm/vite@7.3.0_@types+node@25.0.3_sass@1.77.4_tsx@4.21.0/node_modules/vite/bin/node_modules:/home/tony/Clients/Infonomic/Projects/Payload/payload-alternative-lexical-editor/node_modules/.pnpm/vite@7.3.0_@types+node@25.0.3_sass@1.77.4_tsx@4.21.0/node_modules/vite/node_modules:/home/tony/Clients/Infonomic/Projects/Payload/payload-alternative-lexical-editor/node_modules/.pnpm/vite@7.3.0_@types+node@25.0.3_sass@1.77.4_tsx@4.21.0/node_modules:/home/tony/Clients/Infonomic/Projects/Payload/payload-alternative-lexical-editor/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/tony/Clients/Infonomic/Projects/Payload/payload-alternative-lexical-editor/node_modules/.pnpm/vite@7.3.0_@types+node@25.0.3_sass@1.77.4_tsx@4.21.0/node_modules/vite/bin/node_modules:/home/tony/Clients/Infonomic/Projects/Payload/payload-alternative-lexical-editor/node_modules/.pnpm/vite@7.3.0_@types+node@25.0.3_sass@1.77.4_tsx@4.21.0/node_modules/vite/node_modules:/home/tony/Clients/Infonomic/Projects/Payload/payload-alternative-lexical-editor/node_modules/.pnpm/vite@7.3.0_@types+node@25.0.3_sass@1.77.4_tsx@4.21.0/node_modules:/home/tony/Clients/Infonomic/Projects/Payload/payload-alternative-lexical-editor/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/vite@7.3.0_@types+node@25.0.3_sass@1.77.4_tsx@4.21.0/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/vite@7.3.0_@types+node@25.0.3_sass@1.77.4_tsx@4.21.0/node_modules/vite/bin/vite.js" "$@"
fi
